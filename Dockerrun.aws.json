{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "alexkir1994/client-prod",
      "hostname": "client",
      "essential": false,
      "memory": 64
    },
    {
      "name": "server",
      "image": "alexkir1994/server-prod",
      "hostname": "api",
      "essential": false,
      "memory": 64
    },
    {
      "name": "worker",
      "image": "alexkir1994/worker-prod",
      "hostname": "worker",
      "essential": false,
      "memory": 64
    },
    {
      "name": "nginx",
      "image": "alexkir1994/nginx-prod",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "port": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "client",
        "server"
      ],
      "memory": 128
    }
  ]
}